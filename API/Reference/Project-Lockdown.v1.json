{
  "openapi": "3.0.0",
  "info": {
    "title": "Project Lockdown",
    "version": "1.0",
    "description": "APIs corresponding to Project Lockdown.\nThey serve\n- Back Office (BO)\n- Mapping Platform (MAP)\n- Public Open Data",
    "contact": {
      "name": "The IO Foundation",
      "email": "Contact@ProjectLockdown.net"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/Environments": {
      "get": {
        "summary": "Extract Environment object for MAP.",
        "tags": [
          "ProjectLockdown",
          "TIOF",
          "The IO Foundation",
          "Human Rights",
          "Digital Rights",
          "Nonprofit"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Environment"
                }
              }
            }
          }
        },
        "operationId": "get-Environments",
        "description": "Handles PLD Environment objects.",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "DSLUID",
            "description": "DSLID of the DSL to be loaded. 0== Default DSL / Other value == DSLUID"
          }
        ]
      }
    },
    "/Territories": {
      "get": {
        "summary": "Your GET endpoint",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "operationId": "get-Territories",
        "description": "Handles PLD Territory objects."
      }
    },
    "/DataPoints": {
      "get": {
        "summary": "Your GET endpoint",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        },
        "operationId": "get-DataPoints",
        "description": "Handles PLD DataPoint objects."
      }
    }
  },
  "components": {
    "schemas": {
      "Environment": {
        "title": "Environment",
        "type": "object",
        "x-examples": {
          "example-1": {
            "id": {}
          }
        },
        "description": "Describes the features and behavior of the MAP environment.",
        "properties": {
          "TabMenu": {
            "$ref": "#/components/schemas/TabMenu"
          },
          "TimeSlider": {
            "$ref": "#/components/schemas/TimeSlider"
          }
        },
        "required": [
          "TabMenu",
          "TimeSlider"
        ]
      },
      "TabMenu": {
        "title": "TabMenu",
        "type": "object",
        "description": "Describes the features and behavior of the Time Slider (TS) in MAP.",
        "x-examples": {
          "Example: Default": {
            "id": "1",
            "Show": "True",
            "Position": "Right"
          }
        },
        "properties": {
          "id": {
            "type": "string"
          },
          "Show": {
            "type": "string",
            "description": "  - Description: Controls if the TabMenu is shown. \\n\n  - QS Parameter:  \\n\n  - QS Implemented:  \\n\n  - QS Required:  \\n\n  - Notes:  \\n"
          },
          "Position": {
            "type": "string",
            "description": "  - Description: Indicates in which position the menu is to be rendered.\n  - QS Parameter: TabMenuPosition\n  - QS Implemented: @@@ VERIFY\n  - QS Required: NO\n  - Notes: This parameter can also be set  as per user is left handed or right handed on the Settings (TO BE IMPLEMENTED)"
          }
        }
      },
      "TimeSlider": {
        "title": "TimeSlider",
        "type": "object",
        "description": "Defines the features and behavior of the Time Slider (TS) in MAP.",
        "x-examples": {
          "example-1": {
            "id": "string",
            "Show": "string",
            "Position": "string",
            "StartDate": "string",
            "EndDate": "string",
            "ViewDate": "string"
          }
        },
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "Show": {
            "type": "string",
            "description": "  - Description: Controls if the TimeSlider (TS) is to be rendered.\n  - QS Parameter: TimeSlider -> TimeSliderShow\n  - QS Implemented: YES\n  - QS Required: NO\n  - Notes: @@@ PENDING UPDATE",
            "enum": [
              "True",
              "False"
            ],
            "example": "True"
          },
          "Position": {
            "type": "string",
            "description": "  - Description: Controls where in the map is the Time Slider (TS) to be rendered.\n  - QS Parameter: TimeSliderPosition\n  - QS Implemented: @@@ VERIFY\n  - QS Required: NO\n  - Notes: Only for scalability purposes. There are not plans to render this anywhere else than Bottom.",
            "enum": [
              "Top",
              "Bottom",
              "Left",
              "Right"
            ],
            "example": "Bottom"
          },
          "StartDate": {
            "type": "string",
            "format": "date"
          },
          "EndDate": {
            "type": "string",
            "format": "date"
          },
          "ViewDate": {
            "type": "string",
            "format": "date"
          }
        },
        "required": [
          "id",
          "Show",
          "Position",
          "StartDate",
          "EndDate",
          "ViewDate"
        ]
      },
      "Territory": {
        "title": "Territory",
        "type": "object",
        "description": "Decribes PLD Territories.",
        "x-examples": {
          "example-1": {
            "id": "string",
            "PLDInfo": {
              "id": "string",
              "PermalinkDocs": "string",
              "PLDCode": "string",
              "ParentPLDCode": "string",
              "PermalinkDEI": "string",
              "Name": "string",
              "LastTimeUpdate": "string",
              "Region": "string",
              "Status": "string"
            },
            "ISOInfo": {
              "id": "string"
            },
            "UNInfo": {
              "id": "string"
            },
            "NATOInfo": {
              "id": "string"
            },
            "WikiMediaInfo": {
              "PermalinkDocs": "string",
              "WikidataID": "string"
            },
            "MapboxInfo": {
              "id": "string"
            },
            "PermalinkDocs": "string"
          }
        },
        "properties": {
          "id": {
            "type": "string"
          },
          "PLDInfo": {
            "$ref": "#/components/schemas/PLDInfo"
          },
          "ISOInfo": {
            "$ref": "#/components/schemas/ISOInfo"
          },
          "UNInfo": {
            "$ref": "#/components/schemas/UNInfo"
          },
          "NATOInfo": {
            "$ref": "#/components/schemas/NATOInfo"
          },
          "WikiMediaInfo": {
            "$ref": "#/components/schemas/WikimediaInfo"
          },
          "MapboxInfo": {
            "$ref": "#/components/schemas/MapBoxInfo"
          },
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation explaining this object."
          }
        },
        "required": [
          "id",
          "PLDInfo",
          "MapboxInfo",
          "PermalinkDocs"
        ]
      },
      "DataPoint": {
        "title": "DataPoint",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "description": "Describes PLD Data Points."
      },
      "Category": {
        "title": "Category",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "description": "Describes PLD Categories."
      },
      "DataSetLayer": {
        "title": "DataSet Layer (DSL)",
        "type": "object",
        "properties": {
          "DSLUID": {
            "type": "string"
          },
          "Name": {
            "type": "string"
          }
        },
        "description": "Describes PLD Categories."
      },
      "WikimediaInfo": {
        "title": "WikimediaInfo",
        "type": "object",
        "description": "Describes WikiMedia-related description fields for PLD Territories.",
        "properties": {
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation explaining this object."
          },
          "WikidataID": {
            "type": "string",
            "description": "WikidataID for the Territory."
          }
        },
        "required": [
          "PermalinkDocs",
          "WikidataID"
        ]
      },
      "PLDInfo": {
        "title": "PLDInfo",
        "type": "object",
        "x-examples": {
          "example-1": {
            "id": "string",
            "PermalinkDocs": "string",
            "PLDCode": "string",
            "ParentPLDCode": "string",
            "PermalinkDEI": "string",
            "Name": "string",
            "LastTimeUpdate": "string",
            "Region": "string",
            "Status": "string"
          }
        },
        "description": "Describes PLD-related description fields for PLD Territories.\nInformation originated internally by Project Lockdown.",
        "properties": {
          "UID": {
            "type": "string",
            "description": "UID for this PLDCode object."
          },
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation explaining this object."
          },
          "PLDCode": {
            "type": "string",
            "description": "Internal PLDCode for this Territory."
          },
          "ParentPLDCode": {
            "type": "string",
            "description": "PLDCode of the Territory that contains this Territory."
          },
          "PermalinkDEI": {
            "type": "string",
            "description": "Permalink to the Sheet for this Territory in the DEI."
          },
          "Name": {
            "type": "string",
            "description": "Territory Name (Default ENG)"
          },
          "LastTimeUpdate": {
            "type": "string",
            "description": "Last Date and Time this Territory was updated (UTC+0)"
          },
          "Region": {
            "type": "string",
            "description": "Region to which this Territory belongs."
          },
          "Status": {
            "type": "string",
            "description": "Indicates the status of this Territory in the DEI (see Definitions)"
          },
          "UnitCode": {
            "type": "string",
            "description": "Internal Unit PLDCode for this Territory."
          }
        },
        "required": [
          "UID",
          "PermalinkDocs",
          "PLDCode",
          "ParentPLDCode",
          "PermalinkDEI",
          "Name",
          "LastTimeUpdate",
          "Region",
          "Status",
          "UnitCode"
        ]
      },
      "ISOInfo": {
        "title": "ISOInfo",
        "type": "object",
        "description": "Describes ISO-related description fields for PLD Territories.\nInformation originated by the International Starndards Organization (ISO).",
        "properties": {
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation documenting this object."
          },
          "ISO2": {
            "type": "string",
            "description": "Code ISO Alpha 2 of this Territory (or its parent, if territory is a subdivision)."
          },
          "ISO3": {
            "type": "string",
            "description": "Code ISO Alpha 3 of this Territory (or its parent, if territory is a subdivision)."
          }
        },
        "required": [
          "ISO2",
          "ISO3"
        ]
      },
      "UNInfo": {
        "title": "UNInfo",
        "type": "object",
        "description": "Describes UN-related description fields for PLD Territories.\nInformation originated by United Nations (UN).",
        "properties": {
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation explaining this object."
          },
          "LOCODE": {
            "type": "string",
            "description": "UN LOCODE code for this territory."
          }
        },
        "required": [
          "PermalinkDocs",
          "LOCODE"
        ]
      },
      "NATOInfo": {
        "title": "NATOInfo",
        "type": "object",
        "description": "Describes NATO-related description fields for PLD Territories.\nInformation originated by the North Atlantic Treaty Organization (NATO).",
        "properties": {
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation explaining this object."
          },
          "STANAG": {
            "type": "string",
            "description": "https://en.wikipedia.org/wiki/List_of_NATO_country_codes"
          }
        }
      },
      "MapBoxInfo": {
        "title": "MapBoxInfo",
        "type": "object",
        "description": "Describes Mapbox-related description fields for PLD Territories.\nInformation originated by Mapbox (Map platform provider).",
        "properties": {
          "PermalinkDocs": {
            "type": "string",
            "description": "Permalink to the documentation explaining this object."
          },
          "BoundaryLevel": {
            "type": "string",
            "description": "Boundary Level for this Territory.",
            "enum": [
              "Adm0",
              "Adm1",
              "Adm2",
              "Adm3",
              "Adm4",
              "Adm5"
            ]
          },
          "FeatureID": {
            "type": "string",
            "description": "FeatureID (unique ID) for this Territory."
          }
        },
        "required": [
          "BoundaryLevel",
          "FeatureID"
        ]
      }
    }
  },

 0 && e < completedWeek) {\n          monthDaysArray[i].push({ day: null, label: daysOfTheWeek[e] });\n        } else if (\n          i === roundWeeks - 1 &&\n          excedentDays > 0 &&\n          e > 7 - excedentDays - 1\n        ) {\n          monthDaysArray[i].push({ day: null, label: daysOfTheWeek[e] });\n        } else {\n          days++;\n          monthDaysArray[i].push({ day: days, label: daysOfTheWeek[e] });\n        }\n      }\n    }\n    return monthDaysArray;\n  };\n\n  const fillAllMonths = () => {\n    let prevMonths = allMonthsDate;\n    let firstDay, month;\n    for (let i = 0; i < 12; i++) {\n      if (i !== currentMonth) {\n        firstDay = new Date(2020, i, 1);\n        month = new Date(2020, i + 1, 0);\n        let days = getMonthData(firstDay, month);\n        prevMonths[i] = days;\n      }\n    }\n    setAllMonthsDate(prevMonths);\n    setEnableArrows(true);\n  };\n  const chooseDay = (dayData, week, day) => {\n    let prevData = days;\n    if (lastSelection !== false) {\n      prevData[lastSelection[0]][lastSelection[1]].selected = false;\n    }\n    dayData.selected = true;\n    prevData[week][day] = dayData;\n    props.onSelect(new Date(currentYear, currentMonth, dayData.day));\n    setDays(prevData);\n    setLastSelection([week, day]);\n  };\n  const changeMonth = (plus) => {\n    if (enableArrows) {\n      let newDays, newMonth;\n      if ((currentMonth > 0 || plus) && (currentMonth < 11 || !plus)) {\n        if (plus) {\n          newMonth = currentMonth + 1;\n          newDays = allMonthsDate[newMonth];\n        } else {\n          newMonth = currentMonth - 1;\n          newDays = allMonthsDate[newMonth];\n        }\n        setDays(newDays);\n        setCurrentMonth(newMonth);\n      }\n    }\n  };\n  const daysOfTheWeek = [\"S\", \"M\", \"T\", \"W\", \"T\", \"F\", \"S\"];\n  const monthsNames = [\n    \"January\",\n    \"February\",\n    \"March\",\n    \"April\",\n    \"May\",\n    \"June\",\n    \"July\",\n    \"August\",\n    \"September\",\n    \"October\",\n    \"November\",\n    \"December\",\n  ];\n\n  return (\n    <>\n      <div\n      // height: 600px;\n        className={`overlay ${overlay} ${props.show ? \"shw\" : \"\"}`}\n        onClick={props.close}\n      ></div>\n      <div\n        className={`calendar ${styles} ${props.show ? \"show\" : \"\"} ${\n          props.customClass\n        }`}\n      >\n        <div class=\"calendar actions\">\n          <span onClick={() => changeMonth(false)}> {arrowLeft} </span>\n          <h3>\n            {monthsNames[currentMonth]} {currentYear}\n          </h3>\n          <span onClick={() => changeMonth(true)}> {arrowRight} </span>\n        </div>\n        <div class=\"calendar header\">\n          {daysOfTheWeek.map((dayL) => (\n            <div class=\"header item\">{dayL}</div>\n          ))}\n        </div>\n        <div class=\"calendar container\">\n          {days.map((week, i) => (\n            <div class=\"week\">\n              {week.map((day, e) => (\n                <div\n                  onClick={() => chooseDay(day, i, e)}\n                  class={`day ${day.day === null ? \"empty\" : \"\"} ${\n                    day.selected ? \"selected\" : \"\"\n                  }`}\n                >\n                  <span>{day.day}</span>\n                </div>\n              ))}\n            </div>\n          ))}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default DatePicker;\n","/root/template/ProjectLockdown/MAP/src/assets/icons/icons.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/info.svg.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/settings.svg.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/ticker.svg.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/contribute.svg.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/x.svg.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/true.svg.js",[],"/root/template/ProjectLockdown/MAP/src/assets/icons/lo